														"referenceName": "AzureBlobStorageLogs",
														"type": "LinkedServiceReference"
													},
													"path": "logs/geomapupload"
												}
											}
										},
										"inputs": [
											{
												"referenceName": "zip_geomap_ftp_server",
												"type": "DatasetReference"
